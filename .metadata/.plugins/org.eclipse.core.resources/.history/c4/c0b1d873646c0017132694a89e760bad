package com.evan;

public class HelloWorld {
	public static void main(String[] args) {
		int score = 53;//保存成绩
		int count = 0;//保存加分次数
		System.out.println("加分前成绩： "+score);//打印输出加分前成绩
		for(;score<60;count++)
		{
			score++;
		}
		System.out.println("加分后成绩： "+score);//打印输出加分后成绩
		System.out.println("共加了"+count+"次！");
	}

}
