package com.evan;

public class Telephone {
	float screen;
	float cpu;
	float mem;
	public Telephone() {
		System.out.println("无参的构造方法执行了");
	}
	public Telephone(float newscreen,float newcpu,float newmem) {
		screen = newscreen;
		cpu = newcpu;
		mem = newmem;
		System.out.println("带参的构造方法执行了");
	}
}
