package com.evan;

public class Telephone {
	float screen;
	float cpu;
	float mem;
	public Telephone() {
		System.out.println("无参的构造方法执行了");
	}
	public Telephone(float newscreen,float newcpu,float newmem) {
		if（newscreen < 3.5f）{
			System.out.println("你输入的参数有问题，自动赋值3.5f");
			screen = 3.5f;
		}
		else {
			screen = newscreen;			
		}
		cpu = newcpu;
		mem = newmem;
		System.out.println("带参的构造方法执行了");
	}
}
